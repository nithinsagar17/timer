
 {
    "manifest_version": 2,
    "name": "PRODUCTIVITY MANAGEMENT",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
     
    
    "browser_action": {
      
      "default_popup": "index.html"
      
    }
  }